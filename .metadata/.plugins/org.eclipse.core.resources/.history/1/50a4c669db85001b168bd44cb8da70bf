package estructuras_principales;

public class EjercicioArray2D {

	public static void main(String[] args) {
		double acumulado;
		double interes=0.1;
		
		double [][] saldo = new double[6][5];
		
		
		for(int i=0; i<6; i++) {
			
			saldo[i][0] = 100000;
			acumulado=10000;
			
			for(int j=1; j<5; i++) {
				
				acumulado = acumulado + (acumulado*interes);
				saldo[i][j] = acumulado;
				
				
			}
			
			
			
			
			
		}

	}

}
